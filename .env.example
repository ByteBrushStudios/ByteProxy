# Server Configuration
PORT=3420

# Discord Bot Token (for Discord API proxying)
# Get this from https://discord.com/developers/applications
DISCORD_BOT_TOKEN=your_discord_bot_token_here

# GitHub Personal Access Token (for GitHub API proxying)
# Get this from https://github.com/settings/tokens
GITHUB_TOKEN=your_github_token_here

# API Security Settings
PROXY_API_KEY=your_proxy_api_key_here
MANAGEMENT_API_KEY=your_management_api_key_here
REQUIRE_AUTH_FOR_PROXY=false
REQUIRE_AUTH_FOR_MANAGEMENT=true
